<h1>Cities</h1>

<table>
    <tr>
        <th>City</th>
        <th>code</th>
        <th>Name</th>
        <th>Population</th>
        <th>Coastal</th>
        <th>Area</th>
    </tr>
    <% city.forEach((city) =>
{ %>
    <tr>
        <td><%= city.city_code %></td>
        <td><%= city.co_code %></td>
        <td><%= city.city_name %></td>
        <td><%= city.population %></td>
        <td><%= city.isCoastal %></td>
        <td><%= city.areaKM %></td>
        <td><div id="text" onClick="allDetails()"> All Details </div></td>
    </tr>
    <% }) %>
</table>

<br><br>
<form action="/" class="inline">
    <button class="float-left submit-button" >Home</button>
</form>